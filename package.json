{
  "name": "ngx-contextmenu-platform",
  "version": "6.0.0",
  "description": "An Angular component to show a context menu on an arbitrary component",
  "keywords": [
    "angular2",
    "contextmenu",
    "angular2-contextmenu",
    "ngx-contextmenu",
    "ngx",
    "ng2",
    "ng2-contextmenu"
  ],
  "author": "Isaac Mann <isaacplmann@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/isaacplmann/ngx-contextmenu.git"
  },
  "main": "lib/index.js",
  "module": "lib/index.js",
  "jsnext:main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:lib && npm run build:demo",
    "build:demo": "rimraf dist/demo && ng build your-awesome-library",
    "build:lib": "ng build ngx-contextmenu --configuration production && copyfiles README.md LICENSE dist/ngx-contextmenu/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pub": "npm run pub:lib && npm run pub:demo",
    "pub:demo": "npm run build:demo -- --base-href /ngx-contextmenu/ && gh-pages -d dist/demo",
    "pub:lib": "npm run build:lib && npm publish dist/ngx-contextmenu",
    "pack:lib": "npm run build:lib && cd dist/ngx-contextmenu && npm pack && copyfiles *.tgz ../../ && cd ../.. && rimraf dist/ngx-contextmenu/*.tgz"
  },
  "private": false,
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.4",
    "@angular/cdk": "^18.2.14",
    "@angular/cli": "^19.1.4",
    "@angular/common": "^19.1.3",
    "@angular/compiler": "^19.1.3",
    "@angular/compiler-cli": "^19.1.3",
    "@angular/core": "^19.1.3",
    "@angular/platform-browser": "^19.1.3",
    "@angular/platform-browser-dynamic": "19.1.3",
    "@angular/router": "^19.1.3",
    "@types/node": "~14.6.0",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.3.0",
    "gh-pages": "^3.1.0",
    "jasmine-core": "^4.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^19.1.1",
    "protractor": "^7.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.2",
    "ts-node": "~8.10.2",
    "tsickle": "^0.46.3",
    "tslib": "^2.0.0",
    "tslint": "~6.1.3",
    "typescript": "~5.5.4",
    "zone.js": "~0.15.0"
  },
  "dependencies": {
    "core-js": "^3.26.0"
  }
}